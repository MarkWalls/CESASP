@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>CES Data Upload</h1>   
</div>

<div class="row">
    <div class="col-md-8">
    @using (Html.BeginForm("Upload", "Home", FormMethod.Post, new { EncType="multipart/form-data"}))
        {
            <table>
                <tr>
                    <td>Upload Excel File: </td>
                    <td><input type="text" name="CESFileName" /></td>
                    <td><input type="file" id="ButtonUpload" name="CESFileUpload" /></td>

                </tr>                
                <tr>
                    <td></td>
                    <td align="right"><input type="submit" value="Process" /></td>
                    <td></td>
                </tr>
            </table>
        }
    </div>    
</div>